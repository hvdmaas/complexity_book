  author = {Burger, Julian and Isvoranu, Adela-Maria and Lunansky, Gabriela and Haslbeck, Jonas M. B. and Epskamp, Sacha and Hoekstra, Ria H. A. and Fried, Eiko I. and Borsboom, Denny and Blanken, Tessa F.},
